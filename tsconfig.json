{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2022",
		"outDir": "out",
		"lib": [
			"es2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
